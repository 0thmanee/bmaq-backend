{
    "name": "bmaq-backend",
    "version": "1.0.0",
    "description": "BMAQ Backend API with Fastify, TypeScript, and Prisma",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon --exec tsx src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "db:generate": "prisma generate",
        "db:migrate": "prisma migrate dev",
        "db:deploy": "prisma migrate deploy",
        "db:studio": "prisma studio",
        "db:push": "prisma db push",
        "db:seed": "tsx prisma/seed.ts",
        "lint": "tsc --noEmit",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "fastify",
        "typescript",
        "prisma",
        "postgresql",
        "bmaq"
    ],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "@fastify/cors": "^11.0.1",
        "@fastify/helmet": "^13.0.1",
        "@fastify/rate-limit": "^10.3.0",
        "@fastify/swagger": "^9.5.1",
        "@fastify/swagger-ui": "^5.2.3",
        "@prisma/client": "^6.11.1",
        "axios": "^1.10.0",
        "bcryptjs": "^3.0.2",
        "chalk": "^5.4.1",
        "dotenv": "^17.0.1",
        "fastify": "^5.4.0",
        "fastify-plugin": "^5.0.1",
        "jsonwebtoken": "^9.0.2",
        "pino": "^9.7.0",
        "pino-pretty": "^13.0.0",
        "prisma": "^6.11.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.0.10",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2"
    }
}
